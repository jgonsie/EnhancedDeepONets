FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=64;scalar=64";
    class       volVectorField;
    location    "0";
    object      grad(T);
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -1 0 1 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    left
    {
        type            extrapolatedCalculated;
        value           uniform (0 0 0);
    }
    right
    {
        type            extrapolatedCalculated;
        value           uniform (0 0 0);
    }
    upper
    {
        type            extrapolatedCalculated;
        value           uniform (0 0 0);
    }
    lower
    {
        type            extrapolatedCalculated;
        value           uniform (0 0 0);
    }
    sides
    {
        type            empty;
    }
}


// ************************************************************************* //
